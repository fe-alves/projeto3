<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="./css/style.css">
    <title>Corujão | Catálogo</title>
</head>
<body>
    <article class="imagem">
        <img src="./img/coruja.png" alt="Logo do Site" width="100px" height="100px">
    </article>
    <article>
        <nav>
            <ul class="menu">
                <li><a href="/">Início</a></li>
                <li><a href="/cadastro">Cadastro</a></li>
            </ul>
        </nav>
    </article>
    <section id="catalogo">
        <% filmes.forEach((item) => { %>
            <a class="card" href="/detalhes/<%= item.id %>">
                <img src="<%= item.imagem_filme %>" height="300px"> 
            </a>
        <% }) %>
    </section>
    <script src="./js/script.js"></script>
</body>
</html>
